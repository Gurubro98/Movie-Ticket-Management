<ul class="navbar-nav">
    @if (User.Identity.IsAuthenticated)
    {
        @if (User.IsInRole("Admin"))
        {
            <li class="nav-item">
                <a class="nav-link btn btn-Secondary text-dark" asp-area="" asp-controller="BookingCart" asp-action="BokkedTicket">Booked Ticket</a>
            </li>
            <li class="nav-item">

                <a class="nav-link  text-dark" asp-controller="Admin" asp-action="Index"
                title="Welcome">Welcome @User.Identity.Name</a>
            </li>
            <li class="nav-item">
                <form class="form-inline" asp-controller="Account" asp-action="Logout">
                <button type="submit" class="nav-link btn btn-link text-info">Logout</button>
            </form>
            </li>
        }
        else
        {
            <li class="nav-item">
                <a class="btn btn-Secondary text-dark" asp-area="" asp-controller="BookingCart" asp-action="BokkedTicket">Booked Ticket</a>
            </li>
            <li class="nav-item">

                <a class="nav-link text-dark" asp-controller="User" asp-action="Index"
                   title="Welcome">Welcome @User.Identity.Name</a>
            </li>
            <li class="nav-item">
                <form class="form-inline" asp-controller="Account" asp-action="Logout">
                    <button type="submit" class="nav-link btn btn-link text-info">Logout</button>
                </form>
            </li>
            
        }
    }
    else
    {
        <li class="nav-item">
            <a class="nav-link text-dark" asp-controller="Account"
               asp-action="Login">Login</a>
        </li>
        <li class="nav-item">
            <a class="nav-link text-dark" asp-controller="Account"
               asp-action="Register">Register</a>
        </li>
    }
</ul>